{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Powershell": "Trace",
      "Sample2": "Trace"
    },
    "Console": {
      "LogToStandardErrorThreshold": "Error",
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "LocalFile": {
      "IncludeScopes": true,
      "LogDirectory": "LogFiles",
      "LogFileName": "log_",
      "LogLevel": {
        "Default": "Trace"
      }
    }
  },
  "Host": {
    "ApplicationName": "Brimborium.Werkzeugkasten.Library.Test",
    "EnableLogger": ["Console", "LocalFile"]
  }
}
